using GE_Utilities;
using System.Collections;
using System.Collections.Generic;


namespace EZ_Editor.Models.Enums
{
   public class EN_Resolution: IEnumerable
    {
        private static List<GeEnum<int>> _List;

        //--- _init_list ---------------------
        internal static List<GeEnum<int>> _init_list()
        {
            List<GeEnum<int>> list; 
            list = new List<GeEnum<int>>();
            if (EzGlobals.PrinterCfg.HeadType==Services.EzDef.EPrintHeadType.Norwix_PC22)
            {
                list.Add(new GeEnum<int>(1200, "1200"));
                list.Add(new GeEnum<int>( 600,  "600"));
                list.Add(new GeEnum<int>( 400,  "400"));
                list.Add(new GeEnum<int>( 300,  "300"));
                list.Add(new GeEnum<int>( 240,  "240"));
                list.Add(new GeEnum<int>( 200,  "200"));
                list.Add(new GeEnum<int>( 150,  "150"));
            }
            else if (EzGlobals.PrinterCfg.HeadType==Services.EzDef.EPrintHeadType.EZ_Hydra)
            {
                list.Add(new GeEnum<int>(   100, "100"));
                list.Add(new GeEnum<int>(   150, "150"));
                list.Add(new GeEnum<int>(   200, "200"));
                list.Add(new GeEnum<int>(   300, "300"));
                list.Add(new GeEnum<int>(   400, "400"));
                list.Add(new GeEnum<int>(   141, "141x141 (45°)"));
                list.Add(new GeEnum<int>(   111, "111x224 (63°)"));
                list.Add(new GeEnum<int>(   105, "105x316 (72°)"));
                list.Add(new GeEnum<int>(   103, "103x412 (76°)"));
                list.Add(new GeEnum<int>(   102, "102x510 (79°)"));
                list.Add(new GeEnum<int>(   101, "101x605 (80°)"));
            }    
            else if (EzGlobals.PrinterCfg.HeadType==Services.EzDef.EPrintHeadType.EZ_33
            ||       EzGlobals.PrinterCfg.HeadType==Services.EzDef.EPrintHeadType.EZ_66
            ||       EzGlobals.PrinterCfg.HeadType==Services.EzDef.EPrintHeadType.EZ_100)
            {
                list.Add(new GeEnum<int>( 800,  "800"));
                list.Add(new GeEnum<int>( 600,  "600"));
                list.Add(new GeEnum<int>( 400,  "400"));
                list.Add(new GeEnum<int>( 300,  "300"));
                list.Add(new GeEnum<int>( 150,  "150"));
            }
            else
            {
                list.Add(new GeEnum<int>(1200, "1200"));
                list.Add(new GeEnum<int>( 800,  "800"));
                list.Add(new GeEnum<int>( 600,  "600"));
                list.Add(new GeEnum<int>( 400,  "400"));
                list.Add(new GeEnum<int>( 300,  "300"));
            }

            return list;
        }

        //--- constructor --------------------------------
        public EN_Resolution()
        {
            if (_List==null) _List=_init_list();
        }

        //--- enumerator -----------------------------------
        IEnumerator IEnumerable.GetEnumerator()
        {
            return (IEnumerator)new GeListEnumerator<GeEnum<int>>(_List);
        }

    }
}
