<UserControl x:Class="EZ_Editor.Views.TrackingView.PrinterCtrl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
             xmlns:local="clr-namespace:EZ_Editor.Views.TrackingView"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="50"
             HorizontalAlignment="Left"
             VerticalAlignment="Bottom">

    <Button Width="45"
            Height="45"
            VerticalAlignment="Bottom" 
            HorizontalAlignment="Left" 
            Background="{StaticResource DataGrid.Brush.Selected.Background}" 
            BorderThickness="0"
            >
        <iconPacks:PackIconMaterial Kind="Printer" Margin="10,10,10,5" Foreground="{StaticResource EzEditor.Brush.Dark.Foreground}"/>
    </Button>
</UserControl>
